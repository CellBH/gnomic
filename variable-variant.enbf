
VARIABLE_VARIANT = VARIABLE_VARIANT_IDENTIFIER "=" VARIABLE_VARIANT_VALUE;

VARIABLE_VARIANT_IDENTIFIER = /[a-z0-9][a-zA-Z0-9]*(\.[a-z0-9][a-zA-Z0-9]*)*/;
VARIABLE_VARIANT_VALUE = NUMBER | QUOTED_STRING | UNQUOTED_STRING;

NUMBER = /[-+]?[0-9]*\.?[0-9]+([eE][-+]?[0-9]+)?/;

QUOTED_STRING = /"(?:[^"\\]|\\.)*"/;
UNQUOTED_STRING = /[a-zA-Z0-9]+/; # TODO
